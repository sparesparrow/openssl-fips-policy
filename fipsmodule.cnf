# OpenSSL FIPS 140-3 Module Configuration
# This file contains the FIPS module configuration for OpenSSL 3.3.0

[openssl_init]
providers = provider_sect
alg_section = algorithm_sect

[provider_sect]
fips = fips_sect
base = base_sect

[base_sect]
activate = 1

[fips_sect]
activate = 1
install-version = 1
conditional-errors = 1
security-checks = 1
module-mac = XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX:XX

[algorithm_sect]
# FIPS 140-3 approved algorithms
cipher = AES-256-GCM:AES-256-CBC:AES-256-CTR:AES-128-GCM:AES-128-CBC:AES-128-CTR
digest = SHA3-512:SHA3-384:SHA3-256:SHA-512:SHA-384:SHA-256
mac = HMAC
keyagreement = ECDH
keyencapsulation =
signature = RSA:ECDSA
keymgmt = RSA:EC:HKDF
exchange = ECDH
kem =

# Additional algorithms for backwards compatibility (non-approved)
cipher_compat = AES-256-CBC:AES-128-CBC:DES-EDE3-CBC
digest_compat = SHA-224:SHA-1
signature_compat = DSA
